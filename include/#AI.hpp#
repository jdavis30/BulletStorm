 * Objective:   behaviour script for character objects.
 */

#ifndef AI_H
#define AI_H

class AI {
public:
  AI();
  virtual void update(double dt, Character<AI>* const c);
  int getType();
  
private:
  int type;

};

AI<T>::AI(){
  type = 0;
}

int AI<T>::getType(){
  return type;
}

void AI<T>::update(double dt, Character<AI>* const c) {
  c.setx(c.getx());
}

#endif /*AI_H*/
